	public static void sortArray(int[] array) {
		//int temp;
		int c = 0;
		for (int i = 0; i< array.length; i++) {
			for (int j = i + 1; j < array.length; j++) {
				if (array[i] > array[j]) {
					c++;
//					temp = array[j];
//					array[j] = array[i];
//					array[i] = temp;
					System.out.println(c + " : " + i);
				}
				else {
					continue;
				}
				
			}
		}

	}

}
