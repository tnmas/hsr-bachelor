	public static int sortArray(int[] array) {
		int temp;
		int x;
		for (int i = 0; i< array.length; i++) {
			for (int j = i + 1; j < array.length; j++) {
				if (array[i] > array[j]) {
//					temp = array[j];
//					array[j] = array[i];
//					array[i] = temp;
					x = i;
				}
				else {
					continue;
				}
			}
		}
//		return array;

	}

